    <name>dinCachePlugin</name>
    <channel>plugins.symfony-project.org</channel>
    <summary>Smart cache plugin.</summary>
    <description>Identity management plugin.</description>
    <lead>
        <name>Nicolay N. Zyk</name>
        <user>relo_san</user>
        <email>relo.san@gmail.com</email>
        <active>yes</active>
    </lead>
    <date>2010-07-09</date>
    <version>
        <release>0.9.1</release>
        <api>0.9.1</api>
    </version>
    <stability>
        <release>beta</release>
        <api>beta</api>
    </stability>
    <license uri="http://www.symfony-project.com/license">MIT license</license>
    <notes>-</notes>
    <contents>
        <dir name="/">
            <file role="data" name="README.md" />
            <file role="data" name="README-ru.md" />
            <file role="data" name="LICENSE" />

            <dir name="config">
                <file role="data" name="cache_routing.yml" />
            </dir>

            <dir name="lib">
                <dir name="config">
                    <file role="data" name="dinCacheRoutingConfigHandler.php" />
                    <file role="data" name="dinFactoryConfigHandler.php" />
                </dir>
                <dir name="manager">
                    <file role="data" name="dinCacheManager.php" />
                </dir>
                <dir name="record">
                    <file role="data" name="dinCacheDoctrineListener.php" />
                </dir>
            </dir>
        </dir>
    </contents>

    <dependencies>
        <required>
            <php>
                <min>5.2.4</min>
            </php>
            <pearinstaller>
                <min>1.4.1</min>
            </pearinstaller>
            <package>
                <name>symfony</name>
                <channel>pear.symfony-project.com</channel>
                <min>1.3.0</min>
                <max>1.5.0</max>
                <exclude>1.5.0</exclude>
            </package>
        </required>
    </dependencies>

